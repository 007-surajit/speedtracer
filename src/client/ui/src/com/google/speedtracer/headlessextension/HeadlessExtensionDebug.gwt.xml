<!DOCTYPE document SYSTEM
    "http://google-web-toolkit.googlecode.com/svn/releases/1.7/distro-source/core/src/gwt-module.dtd">
<module rename-to="speedtracerheadless">
  <inherits name="com.google.speedtracer.headlessextension.HeadlessExtension" />

  <!-- Human readable Style names -->
  <set-configuration-property name="CssResource.style" value="pretty" />
  <set-property name='speedtracer.use_debug_mode' value='please' />
</module>
