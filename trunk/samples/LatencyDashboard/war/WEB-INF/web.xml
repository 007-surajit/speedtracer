<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
    <servlet> <servlet-name>timelineServlet</servlet-name>
    <servlet-class>com.google.speedtracer.latencydashboard.server.TimelineServiceImpl</servlet-class>
    </servlet>
    
    <servlet-mapping> <servlet-name>timelineServlet</servlet-name>
    <url-pattern>/latencydashboard/timeline</url-pattern>
    </servlet-mapping>

  <servlet>
    <servlet-name>speedTracerReceiver</servlet-name>
    <servlet-class>com.google.speedtracer.latencydashboard.server.SpeedTraceReceiverServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>speedTracerReceiver</servlet-name>
    <url-pattern>/latencydashboard/speedtracereceiver</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>LatencyDashboard.html</welcome-file>
  </welcome-file-list>

</web-app>
