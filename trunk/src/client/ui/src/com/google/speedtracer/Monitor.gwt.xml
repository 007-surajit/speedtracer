<!DOCTYPE document SYSTEM
    "http://google-web-toolkit.googlecode.com/svn/releases/1.7/distro-source/core/src/gwt-module.dtd">
<module rename-to="monitor">
  <inherits name="com.google.speedtracer.Common" />
  <inherits name='com.google.gwt.xhr.XMLHttpRequest' />
  <entry-point class="com.google.speedtracer.client.Monitor" />
  <public path="monitor/public" />
  <!-- TODO(knorton): Moving away from the iframe linker as it no longer works in chrome extensions. -->
  <add-linker name="xs"/>
</module>
